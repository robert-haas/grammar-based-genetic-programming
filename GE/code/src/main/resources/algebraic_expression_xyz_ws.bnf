<expr> ::= ( <var> <op> <var> ) <op> <var> | <var> <op> ( <var> <op> <var> )
<var> ::= x | y | z | <const>
<op> ::= + | - | * | /
<const> ::= <float> | <int>
<int> ::= <nonzero> | <nonzero> <digit>
<float> ::= <digit> . <digit> <digit> <digit>
<digit> ::= 0 | <nonzero>
<nonzero> ::= 1 | 2 | 3 | 4 | 5 | 6 | 7 | 8 | 9
